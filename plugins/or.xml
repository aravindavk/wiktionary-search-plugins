<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <Author>Aravinda VK, mail@aravindavk.in</Author>
  <License>MIT</License>
  <ShortName>Wiktionary (or)</ShortName>
  <Description>Oriya Wiktionary Search Plugin</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">data:image/icon;base64,</Image>
  <Url type="application/x-suggestions+json" method="GET" template="http://or.wiktionary.org/w/api.php">
    <Param name="action" value="opensearch"/>
    <Param name="search" value="{searchTerms}"/>
  </Url>
  <Url type="text/html" method="GET" template="http://or.wiktionary.org/wiki/Special:Search">
    <Param name="search" value="{searchTerms}"/>
    <Param name="sourceid" value="Mozilla-search"/>
  </Url>
  <SearchForm>http://or.wiktionary.org/wiki/Special:Search</SearchForm>
</OpenSearchDescription>